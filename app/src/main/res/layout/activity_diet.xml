<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout_header"
        android:layout_width="416dp"
        android:layout_height="103dp"
        android:background="@color/background"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textapp2"
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:fontFamily="@font/itimregular"
                android:gravity="center"
                android:text="FITCALC"
                android:textColor="@color/red"
                android:textSize="36sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/linearLayout7"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/background"
                android:orientation="horizontal"
                android:layout_marginTop="-10dp">

                <TextView
                    android:id="@+id/textView_yesterday"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="36dp"
                    android:text="1" />

                <TextView
                    android:id="@+id/textView_today"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="36dp"
                    android:text="2" />

                <TextView
                    android:id="@+id/textView_tomorrow"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="36dp"
                    android:text="3" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="413dp"
        android:layout_height="99dp"
        android:background="@color/lightbackground"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout_L"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">


        <TextView
            android:id="@+id/textview_sniadanie"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/itimregular"
            android:gravity="center"
            android:text="Åšniadanie"
            android:textColor="@color/black"
            android:textSize="21sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_sniadanie_kcal"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_sniadanie_b"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_sniadanie_t"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_sniadanie_w"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <Button
                android:id="@+id/textview_sniadanie_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="-4dp"

                android:layout_marginRight="15dp"
                android:background="@drawable/add" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_L"
        android:layout_width="413dp"
        android:layout_height="99dp"
        android:background="@color/background"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout_O"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textview_lunch"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/itimregular"
            android:gravity="center"
            android:text="Lunch"
            android:textColor="@color/black"
            android:textSize="21sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_lunch_kcal"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="TextView"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_lunch_b"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="TextView"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_lunch_t"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="TextView"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_lunch_w"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="TextView"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/textview_lunch_button"
                android:layout_width="40dp"
                android:layout_height="40dp"

                android:layout_marginRight="15dp"
                android:layout_marginTop="-4dp"
                android:background="@drawable/add" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_O"
        android:layout_width="413dp"
        android:layout_height="99dp"
        android:background="@color/lightbackground"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout_K"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textview_obiad"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/itimregular"
            android:gravity="center"
            android:text="Obiad"
            android:textColor="@color/black"
            android:textSize="21sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_obiad_kcal"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_obiad_b"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_obiad_t"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_obiad_w"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <Button
                android:id="@+id/textview_obiad_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="-4dp"
                android:layout_marginRight="15dp"
                android:background="@drawable/add" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_K"
        android:layout_width="412dp"
        android:layout_height="104dp"
        android:background="@color/background"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout_P"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textview_kolacja"
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="-3dp"
            android:gravity="center"
            android:fontFamily="@font/itimregular"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:text="Kolacja" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_kolacja_kcal"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_kolacja_b"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_kolacja_t"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_kolacja_w"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <Button
                android:id="@+id/textview_kolacja_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="-4dp"

                android:layout_marginRight="15dp"
                android:background="@drawable/add" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_P"
        android:layout_width="421dp"
        android:layout_height="102dp"
        android:background="@color/lightbackground"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout_Footer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textview_podsumowanie"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/itimregular"
            android:gravity="center"
            android:text="Podsumowanie dnia"
            android:textColor="@color/black"
            android:textSize="21sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_podsumowanie_kcal"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="45dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_podsumowanie_b"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_podsumowanie_t"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />

            <TextView
                android:id="@+id/textview_podsumowanie_w"
                android:layout_width="1dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:textSize="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:text="TextView" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_Footer"
        android:layout_width="416dp"
        android:layout_height="64dp"
        android:background="@color/background"
        android:gravity="center"
        android:orientation="horizontal"
        android:weightSum="4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/jedzenie" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/analiza" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/stopa" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ludzik" />
    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>